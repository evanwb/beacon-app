{
  "expo": {
    "name": "Beacon",
    "slug": "beacon-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ed3024"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iridium.beacon",
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "com.iridium.beacon",
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Trap-Black.otf",
            "./assets/fonts/Trap-Bold.otf"
          ]
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ],
      [
        "react-native-wifi-reborn",
        {
          "fineLocationPermission": false
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],["expo-dynamic-app-icon",
      {
        "red": { 
          "image": "./assets/icon.png", 
          "prerendered": true 
          
        },
        "alt": {
          "image": "./assets/icon2.png",
          "prerendered": true
        }
      }]
      
    ],
    "extra": {
      "eas": {
        "projectId": "54ab47f9-cd15-4418-b6de-964f725596c6"
      }
    }
  }
}
